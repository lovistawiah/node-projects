<!-- include header -->
<%- include('./partials/_header')%>
    <!-- include header -->
    <main id="site-main">
        <div class="container">
            <div class="box-nav d-flex justify-between">
                <a href="/add-user" class="border-shadow">
                    <span class="text-gradient"><i class="fas fa-user"></i> New User</span>
                </a>
            </div>
            <!-- form handling -->
            <form action="/" method="post">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th>ID</th>
                            <th>Name</th>
                            <th>@Email</th>
                            <th>Gender</th>
                            <th>Status</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%- include('./partials/_show')%>

                    </tbody>
                </table>
            </form>
        </div>
    </main>
    <!-- include footer -->
    <%- include('./partials/_footer')%>
