<%for(var i=0; i< users.length; i++){%>
<tr>
    <td><%= i+1%></td>
    <td><%= users[i].name%></td>
    <td><%= users[i].email%></td>
    <td><%= users[i].gender%></td>
    <td><%= users[i].status%></td>
    <td>
        <a href="/user/update?id=<%=users[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient">
                <i class="fas fa-pencil-alt"></i>
            </span>
        </a>
        <a href="/user/delete" class="btn border-shadow delete">
            <span class="text-gradient">
                <i class="fas fa-trash"></i>
            </span>
        </a>

    </td>
</tr>
<%}%>